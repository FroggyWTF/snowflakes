{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "dot-notation": "off",
        "max-len": "off",
        "no-useless-return": "off",
        "consistent-return": "off"
    }
}
